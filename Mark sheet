        if (marks[2] < 30 ) {

            System.out.println("You Got Fail Grade in Your Subject" + " " + sub[2]);
        }
        if (marks[3] >= 80) {

            System.out.println("You Got A+ Grade in Your Subject" + " " + sub[3]);
        }
        if (marks[3] >= 70 && marks[3] <80) {

            System.out.println("You Got A Grade in Your Subject" + " " + sub[3]);
