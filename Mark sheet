import java.util.Scanner;

public class Marks {

    public static void main(String[] args) {

        int marks[] = new int[6];
        Scanner Obj = new Scanner(System.in);
        String sub[] = {"English", "Urdu", "Math", "Physics", "Chem", "Isl"};
        for (int i = 0; i < marks.length; i++) {
            System.out.println("Enter Marks of Your Subject" + " " + sub[i]);
            marks[i] = Obj.nextInt();
        }
        System.out.println("Grade Calculation");
        if (marks[0] >= 80) {

            System.out.println("You Got A+ Grade in Your Subject" + " " + sub[0]);
        }
        if (marks[0] >= 70 && marks[0] <80) {

            System.out.println("You Got A Grade in Your Subject" + " " + sub[0]);
        }
        if (marks[0] >= 60 && marks[0] <70) {

            System.out.println("You Got B Grade in Your Subject" + " " + sub[0]);
        }
        if (marks[0] >= 50 && marks[0] <60) {

            System.out.println("You Got C Grade in Your Subject" + " " + sub[0]);
        }
        if (marks[0] >= 40 && marks[0] <50) {

            System.out.println("You Got D Grade in Your Subject" + " " + sub[0]);
        }
        if (marks[0] >= 30 && marks[0] <40) {

            System.out.println("You Got E Grade in Your Subject" + " " + sub[0]);
        }
        if (marks[0] < 30 ) {

            System.out.println("You Got Fail Grade in Your Subject" + " " + sub[0]);
        }

        if (marks[1] >= 80) {

            System.out.println("You Got A+ Grade in Your Subject" + " " + sub[1]);
